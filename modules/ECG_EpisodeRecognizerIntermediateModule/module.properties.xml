<?xml version="1.0"?>
<module>
	<id>
		org.electrocodeogram.module.intermediate.implementation.EpisodeRecognizerIntermediateModule
	</id>
	<provider-name>
		FU Berlin
	</provider-name>
	<version>
		1.0
	</version>
	<name>
		Simple episode recognizer
	</name>
	<class>
		org.electrocodeogram.module.intermediate.implementation.EpisodeRecognizerIntermediateModule
	</class>
	<description>
		This module is used as a shell of one configurable EpisodeRecognizer. You can 
		specify the Recognizer in the property "Recognizer". The module also contains some
		new MSDTs which actually are episode representations.
		
		The properties are:
		* Recognizer - The name of the class to be found in the ...recognizer package
		* MinDuration - No Episode shorter than MinDuration (in ms) should be recognized
	</description>
	<type>
		INTERMEDIATE
	</type>
	<properties>
		<property>
			<propertyName>
				Recognizer
			</propertyName>
			<propertyType>
				java.lang.String
			</propertyType>
			<propertyValue>
			</propertyValue>
		</property>
		<property>
			<propertyName>
				MinDuration
			</propertyName>
			<propertyType>
				java.lang.Long
			</propertyType>
			<propertyValue>
			</propertyValue>
		</property>
	</properties>
	<microsensordatatypes>
		<microsensordatatype>
			<msdtName>
				msdt.fileactive.xsd
			</msdtName>
			<msdtFile>
				msdt.fileactive.xsd
			</msdtFile>
		</microsensordatatype>
		<microsensordatatype>
			<msdtName>
				msdt.windowactive.xsd
			</msdtName>
			<msdtFile>
				msdt.windowactive.xsd
			</msdtFile>
		</microsensordatatype>
		<microsensordatatype>
			<msdtName>
				msdt.partactive.xsd
			</msdtName>
			<msdtFile>
				msdt.partactive.xsd
			</msdtFile>
		</microsensordatatype>
		<microsensordatatype>
			<msdtName>
				msdt.runactive.xsd
			</msdtName>
			<msdtFile>
				msdt.runactive.xsd
			</msdtFile>
		</microsensordatatype>
		<microsensordatatype>
			<msdtName>
				msdt.antactive.xsd
			</msdtName>
			<msdtFile>
				msdt.antactive.xsd
			</msdtFile>
		</microsensordatatype>
		<microsensordatatype>
			<msdtName>
				msdt.cpcwarning.xsd
			</msdtName>
			<msdtFile>
				msdt.cpcwarning.xsd
			</msdtFile>
		</microsensordatatype>
	</microsensordatatypes>
</module>

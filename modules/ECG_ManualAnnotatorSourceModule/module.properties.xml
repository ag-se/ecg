<?xml version="1.0"?>
<module>
	<id>
		org.electrocodeogram.module.source.implementation.ManualAnnotatorSourceModule
	</id>
	<provider-name>
		Frank Schlesinger (Frank@Schlesinger.com)
	</provider-name>
	<version>
		1.0
	</version>
	<name>
		Manual Annotator
	</name>
	<class>
		org.electrocodeogram.module.source.implementation.ManualAnnotatorSourceModule
	</class>
	<description>
		Provides a dialog to the user with buttons to generate events manually. Each button
		is according to one event.
	</description>
	<type>
		SOURCE
	</type>
	<properties>
		<property>
			<propertyName>
				Show Dialog
			</propertyName>
			<propertyType>
				java.lang.Boolean
			</propertyType>
			<propertyValue>
				True
			</propertyValue>
		</property>
		<property>
			<propertyName>
				Events
			</propertyName>
			<propertyType>
				org.electrocodeogram.module.source.implementation.ManualAnnotatorEvents
			</propertyType>
			<propertyValue>
				Defekt erkannt
			</propertyValue>
		</property>
		<property>
			<propertyName>
				Episodes
			</propertyName>
			<propertyType>
				org.electrocodeogram.module.source.implementation.ManualAnnotatorEpisodes
			</propertyType>
			<propertyValue>
				Pause
			</propertyValue>
		</property>
	</properties>
	<microsensordatatypes>
		<microsensordatatype>
			<msdtName>
				msdt.manualevent.xsd
			</msdtName>
			<msdtFile>
				msdt.manualevent.xsd
			</msdtFile>
		</microsensordatatype>
		<microsensordatatype>
			<msdtName>
				msdt.manualepisode.xsd
			</msdtName>
			<msdtFile>
				msdt.manualepisode.xsd
			</msdtFile>
		</microsensordatatype>
	</microsensordatatypes>
</module>
